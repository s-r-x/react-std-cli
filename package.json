{
  "name": "@strax77/reactcmd",
  "version": "1.1.0",
  "description": "Missing react cli",
  "bin": {
    "reactcmd": "./build/cli/index.js"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "dev": "tsc -p ./tsconfig.dev.json --watch",
    "lint": "eslint --ext .ts lib",
    "lint.strict": "eslint --ext .ts lib --max-warnings 0",
    "lint.fix": "eslint --ext .ts --fix lib",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha -r ts-node/register -r tests/mocha-hooks.ts \"lib/**/*spec.ts\" \"tests/**/*spec.ts\"",
    "semantic-release": "semantic-release"
  },
  "author": "Ilya Strus <strax1990@gmail.com>",
  "keywords": [
    "react",
    "cli",
    "react-cli"
  ],
  "engines": {
    "node": ">=12"
  },
  "bugs": {
    "url": "https://github.com/s-r-x/reactcmd/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/s-r-x/reactcmd.git"
  },
  "homepage": "https://github.com/s-r-x/reactcmd",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/jscodeshift": "^0.11.2",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "@types/prettier": "^2.4.1",
    "@types/sinon-chai": "^3.2.5",
    "@types/tmp": "^0.2.1",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.2",
    "semantic-release": "^18.0.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "tmp": "^0.0.33",
    "ts-node": "^10.2.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "chalk": "^4.1.2",
    "cosmiconfig": "^7.0.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.5",
    "inversify": "^6.0.1",
    "jscodeshift": "^0.13.0",
    "lodash": "^4.17.21",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.4.1",
    "pretty-error": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "typescript-memoize": "^1.0.1",
    "yargs": "^17.2.1"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
